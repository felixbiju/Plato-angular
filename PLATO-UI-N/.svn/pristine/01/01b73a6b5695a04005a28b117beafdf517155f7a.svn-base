<div class="w3-modal modal-lg" id="sonarreportpopup" style="display: none" >
  <div class="w3-modal-content w3-animate-zoom" id="modalContainer">

      <header class="w3-container">
      <a onclick="document.getElementById('sonarreportpopup').style.display='none'" class="modal-close closebutton">
          <svg viewBox="0 0 24 24">
            <path d="M19 6.41l-1.41-1.41-5.59 5.59-5.59-5.59-1.41 1.41 5.59 5.59-5.59 5.59 1.41 1.41 5.59-5.59 5.59 5.59 1.41-1.41-5.59-5.59z"
            />
            <path d="M0 0h24v24h-24z" fill="none" />
          </svg>
        </a>
        <h2 class="headingfont">SONAR REPORT</h2>
      </header>
      <div class="w3-container modal-content customScroll">
        <div style="display: block">
          <mat-card class="col-md-4 sonarcard">
            <h6>Issues</h6>
            <canvas baseChart
            [datasets]="[{data:issuesbarChartData,label:'Status'}]"
            [colors]="barChartColors"
            [labels]="issuesbarChartLabels"
            [options]="barChartOptions"
            [chartType]="barChartType"></canvas>
          </mat-card>
        </div>

        <div style="display: block">
          <mat-card class="col-md-4 sonarcard"><h6>Complexity</h6>
            <canvas baseChart
            [datasets]="[{data:complexitybarChartData,label:'Status'}]"
            [colors]="barChartColors"
            [labels]="complexitybarChartLabels"
            [options]="barChartOptions"
            [chartType]="barChartType"></canvas>
          </mat-card>
        </div>

        <div style="display: block">
          <mat-card class="col-md-4 sonarcard"><h6>Duplications</h6>
            <canvas baseChart
            [datasets]="[{data:duplicationsbarChartData,label:'Status'}]"
            [colors]="barChartColors"
            [labels]="duplicationsbarChartLabels"
            [options]="barChartOptions"
            [chartType]="barChartType"></canvas>
          </mat-card>
        </div>
      </div>
      <footer class="w3-container">
        <button class="btn mat-raised-button mat-primary" color="primary" mat-raised-button
        onclick="document.getElementById('sonarreportpopup').style.display='none'">
          Close
        </button>
      </footer>

  </div>
</div>


  <div class="w3-modal modal-lg" id="junitreportpopup" style="display:none;" >
      <div class="w3-modal-content w3-animate-zoom" id="modalContainer">
            <header class="w3-container">
                <a onclick="document.getElementById('junitreportpopup').style.display='none'" class="modal-close closebutton">
                  <svg viewBox="0 0 24 24">
                    <path d="M19 6.41l-1.41-1.41-5.59 5.59-5.59-5.59-1.41 1.41 5.59 5.59-5.59 5.59 1.41 1.41 5.59-5.59 5.59 5.59 1.41-1.41-5.59-5.59z"
                    />
                    <path d="M0 0h24v24h-24z" fill="none" />
                  </svg>
                </a>
                <h2 class="headingfont">UNIT TESTING REPORT</h2>
              </header>
                <div class="w3-container modal-content customScroll">
                    <div class="row">
                    <div *ngFor="let chart of chartDataJunit" i="index" style="display: block">
                    <mat-card class="col-md-6 sonarcard">
                        <h6>{{chart.test_name}}</h6>
                        <canvas baseChart height="95"
                        [datasets]="[{data:[chart.errors,chart.passed,chart.failures,chart.skipped], label:'Status'}]"
                        [colors]="testsbarChartColors"
                        [labels]="barChartLabels"
                        [options]="barChartOptions"
                        [chartType]="barChartType"></canvas>
                    </mat-card>
                  </div>
                </div>
          </div>
    <footer class="w3-container">
        <!-- <button class="btn mat-raised-button mat-primary" color="primary" mat-raised-button onclick="document.getElementById('confirmDelete01').style.display='none'">
          Cancel
        </button> -->
        <button class="btn mat-raised-button mat-primary" color="primary" mat-raised-button
        onclick="document.getElementById('junitreportpopup').style.display='none'">
          Close
        </button>
      </footer>

    </div>
    </div>

    <div id="createTask2" style="display:none;margin-left:8%;padding:5%" >
      <div  *ngFor="let chart of chartDataTestNg" i="index" style="display: block;">
        <mat-card class="col-md-6">
            <h6>{{chart.test_name}}</h6>
            <canvas baseChart height="95"
            [datasets]="[{data:[chart.errors,chart.passed,chart.failures,chart.skipped], label:'Status'}]"
            [colors]="testsbarChartColors"
            [labels]="barChartLabels"
            [options]="barChartOptions"
            [chartType]="barChartType"></canvas>
        </mat-card>
      </div>
    </div>

    <div class="w3-modal modal-lg" id="duckcreekreportpopup" style="display:none;" >
      <div class="w3-modal-content w3-animate-zoom" id="modalContainer">

            <header class="w3-container">
                <a onclick="document.getElementById('duckcreekreportpopup').style.display='none'" class="modal-close closebutton">
                  <svg viewBox="0 0 24 24">
                    <path d="M19 6.41l-1.41-1.41-5.59 5.59-5.59-5.59-1.41 1.41 5.59 5.59-5.59 5.59 1.41 1.41 5.59-5.59 5.59 5.59 1.41-1.41-5.59-5.59z"
                    />
                    <path d="M0 0h24v24h-24z" fill="none" />
                  </svg>
                </a>
                <h2 class="headingfont">{{'Duckcreek Manuscript Analyzer Report'|uppercase}}</h2>
              </header>
                <div class="w3-container modal-content customScroll">
                  <table class="plato_table" style="display: block;">
                      <ng-container *ngFor="let tabledata of duckcreektable | tool">
                        <!-- {{endtoend.key}} -->
                          <ng-container *ngIf="tabledata.key === 'tabular_data'">
                            <!-- {{endtoend.value[0] | json}} -->
                            <table>
                                    <tr>
                                    <th *ngFor="let eachtabledata of tabledata.value[0] | tool">{{eachtabledata.key}} </th>
                                    </tr>

                                    <tr *ngFor="let data of tabledata.value; let i = index">
                                        <td *ngFor="let tablecoldata of tabledata.value[i] |tool " width="10%">{{tablecoldata.value}}</td>
                                    </tr>
                            </table>
                          </ng-container>
                          </ng-container>

                    </table>
          </div>
    <footer class="w3-container">
      <button class="btn mat-raised-button mat-primary" color="primary" mat-raised-button
      onclick="document.getElementById('duckcreekreportpopup').style.display='none'">
        Close
      </button>
    </footer>

  </div>
</div>

