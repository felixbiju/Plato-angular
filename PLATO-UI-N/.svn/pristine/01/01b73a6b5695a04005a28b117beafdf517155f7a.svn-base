  <!-- static code analysis -->
  <div class="row" *ngIf="staticCodeReport">
    <div class="col-sm-12">
      <div  class="myheading">
        <span class="mytitle">
          SONAR
          <a target="_blank" [href]="sonar_url" style="margin: 0 1rem; color: white;">
            Static Code Analysis
          </a>
        </span>
        <span class="mytitle" style="float: right;">
          <label for="TestRight"> TestRight:</label>
          <a target="_blank" [href]="testright_engine_url" style="margin: 0 1rem; color: white;">
            <i class="fas fa-cogs" style="margin: 0 8px;font-size: 18px;"></i>
          </a>
          <a target="_blank" [href]="testright_analysis_url" style="margin: 0 1rem; color: white;">
            <i class="fas fa-chart-line" style="margin: 0 8px;font-size: 18px;"></i>
          </a>
        </span>
        <img class="min-max_button"
        (click)="hideandshowreport('chartsDiv')"
        src="../../assets/genral_icons/minus_sm.png"
         />
      </div>
    </div>
  </div>

  <div class="row" id="chartsDiv" style="display:block;">
    <div *ngIf="staticCodeReport" class="col-md-12">
      <mat-card class="technicaldebtcard">
        <h6 class="graphfont"> Static Code Analysis </h6>
        <canvas baseChart height="110" onclick="document.getElementById('sonarreportpopup').style.display='block'"
          [datasets]="[{data:SCRbarChartData,label:'Status'}]"
          [colors]="SCRbarChartColors"
          [labels]="SCRbarChartLabels"
          [options]="barChartOptions"
          [chartType]="barChartType"></canvas>
      </mat-card>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <div *ngIf="duckcreekReport" class="myheading">
         <span class="mytitle"> Duckcreek Manuscript Analyzer</span>
        <img class="min-max_button"
        (click)="hideandshowDuckcreekreport('DuckcreekchartsDiv')"
        src="../../assets/genral_icons/minus_sm.png"
         />
      </div>
    </div>
  </div>
  <div class="row" id="DuckcreekchartsDiv" style="display:block;">
    <div *ngIf="duckcreekReport" class="col-md-12" onclick="document.getElementById('duckcreekreportpopup').style.display='block'">
      <mat-card class="technicaldebtcard">
        <h6 class="graphfont"> Duckcreek Manuscript Analyzer </h6>
        <canvas baseChart height="110"
          [datasets]="[{data:duckcreekbarChartData,label:'Status'}]"
          [colors]="duckcreekbarChartColors"
          [labels]="duckcreekbarChartLabels"
          [options]="barChartOptions"
          [chartType]="barChartType"></canvas>
      </mat-card>
    </div>
  </div>
  <div class="row">
      <div class="col-sm-12">
        <div *ngIf="chartDataJunit || chartDataTestNg" class="myheading">
           <span class="mytitle">Unit Testing</span>
          <img class="min-max_button"
          (click)="hideandshowtestreport('unitTestDiv')"
          src="../../assets/genral_icons/minus_sm.png"
           />
        </div>
      </div>
  </div>
  <div class="row" id="unitTestDiv" style="display:block;">
    <div *ngIf="chartDataJunit" class="col-md-12" style="display: block" onclick="document.getElementById('junitreportpopup').style.display='block'">
      <mat-card class="technicaldebtcard">
        <h6 class="graphfont">JUnit Testing</h6>
        <canvas baseChart height="110"
        [datasets]="[{data:testsbarChartData,label:'Status'}]"
        [colors]="testsbarChartColors"
        [labels]="testsbarChartLabels"
        [options]="barChartOptions"
        [chartType]="barChartType"></canvas>
      </mat-card>
    </div>
  </div>

  <div class="row" id="unitTestDiv" style="display:block;">
    <div *ngIf="chartDataTestNg" class="col-md-12" style="display: block" onclick="document.getElementById('createTask2').style.display='block';document.getElementById('createTask').style.display='none' ">
      <mat-card class="technicaldebtcard">
        <h6 class="graphfont">TestNg Testing</h6>
        <canvas baseChart height="110"
        [datasets]="[{data:testsbarChartData,label:'Status'}]"
        [colors]="testsbarChartColors"
        [labels]="testsbarChartLabels"
        [options]="barChartOptions"
        [chartType]="barChartType"></canvas>
      </mat-card>
    </div>
  </div>

</div>




