.panel-blue .panel-heading,
.panel-green .panel-heading,
.panel-yellow .panel-heading,
.panel-red .panel-heading {
    border: none;
    border-radius: 0;
    margin: 5px 6px -2px 0px;
}

.panel-blue {
    color: #fff;
    background-color: transparent;
}

.panel-blue .panel-heading {
    margin-left: 15px;
    background-color: #1192d4;
}

.panel-green .panel-heading {
    color: #fff;
    background-color: #00a65a;
}

.panel-red .panel-heading {
